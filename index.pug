        include ./slides/caching/cache-on-response.pug
        include ./slides/caching/network-first.pug 
        include ./slides/caching/cache-first.pug 
        include ./slides/caching/race.pug       
        include ./slides/caching/cache-only.pug 
        include ./slides/caching/network-only.pug 
        include ./slides/caching/caching-strategies.pug 

        include ./slides/data/data.pug 
        include ./slides/data/cookies.pug 
        include ./slides/data/html5-storage.pug 
        include ./slides/data/websql.pug 
        include ./slides/data/indexedDB.pug 
        include ./slides/data/file-api.pug 
         
     
